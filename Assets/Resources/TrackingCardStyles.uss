#TrackingCardNavContainer {
    flex-grow: 1;
    background-color: rgb(99, 96, 92);
}

#TrackingCardTitleLabel {
    -unity-font-style: bold;
    color: rgb(255, 255, 255);
    background-color: rgb(34, 34, 34);
    padding: 3px;
}

#TrackingCardContainer {
    flex-direction: column;
    padding: 5%;
    display: flex;
    background-color: rgba(34,34,34,0);
    width: 98%;
}

.Card {
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
    border-radius: 5%;
    border-width: 1%;
    padding: 2%;
    margin: 2%;
    height: 20%;
    width: 98%;
}

.CardLevel {
    color: rgb(255, 255, 255);
    padding: 0;
    -unity-text-align: middle-center;
    margin:0 2%;
}

.CardImage {
    width: 30px;
    height: 30px;
    border-radius: 20px;
    background-size: 100% 100%;
    justify-content: center;
    align-items: center;
}

.CardName {
    color: rgb(255, 255, 255);
    padding: 0;
    -unity-text-align: middle-center;
    margin:0 2%;
}
